{
  "name": "super-client-r",
  "version": "0.0.1",
  "description": "A super client application built with Electron, React and Ant Design",
  "author": "Mark",
  "private": true,
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "packageManager": "pnpm@10.24.0",
  "main": "out/main/main.js",
  "scripts": {
    "dev": "tsc -b && electron-vite dev",
    "build": "tsc -b && electron-vite build && electron-builder",
    "build:mac": "tsc -b && electron-vite build && electron-builder --mac",
    "build:icons": "tsx scripts/build-icons.ts",
    "lint": "oxlint .",
    "lint:biome": "biome lint .",
    "format": "biome format --write .",
    "check:biome": "biome check --write .",
    "preview": "electron-vite preview",
    "check": "tsc -b --noEmit",
    "i18n:check": "tsx scripts/i18n/check.ts",
    "i18n:translate": "tsx scripts/i18n/translate.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/x": "^2.1.3",
    "@anthropic-ai/claude-agent-sdk": "^0.2.19",
    "@anthropic-ai/sdk": "^0.71.2",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^15.3.0",
    "@modelcontextprotocol/sdk": "^1.25.3",
    "@types/react-window": "^2.0.0",
    "antd": "^6.2.1",
    "axios": "^1.13.5",
    "clsx": "^2.1.1",
    "electron-store": "^11.0.2",
    "get-port": "^7.1.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "koa": "^3.1.1",
    "koa-bodyparser": "^4.4.1",
    "koa-proxies": "^0.12.4",
    "koa-router": "^14.0.0",
    "koa-swagger-decorator": "^1.8.7",
    "lucide-react": "^0.563.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-i18next": "^16.5.3",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "react-window": "^2.2.6",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "swagger-ui-dist": "^5.31.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.12",
    "@tailwindcss/vite": "^4.1.18",
    "@types/koa": "^3.0.1",
    "@types/koa-bodyparser": "^4.3.13",
    "@types/koa-router": "^7.4.9",
    "@types/koa__cors": "^5.0.1",
    "@types/koa__router": "^12.0.5",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.6",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "electron": "^38.7.2",
    "electron-builder": "^26.4.0",
    "electron-vite": "^5.0.0",
    "jsdom": "^25.0.1",
    "oxlint": "^1.41.0",
    "postcss": "^8.5.6",
    "rolldown": "1.0.0-rc.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "~5.8.3",
    "vite": "^7.3.1",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^2.1.9"
  },
  "build": {
    "appId": "com.superclient.app",
    "productName": "Super Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "out/**/*",
      "build/icons/**/*"
    ],
    "mac": {
      "icon": "build/icons/icon.icns",
      "category": "public.app-category.productivity",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ]
    },
    "dmg": {
      "icon": "build/icons/icon.icns",
      "iconSize": 128,
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}