{
  "name": "iconfont-downloader-skill",
  "version": "2.0.0",
  "description": "从iconfont.cn搜索并下载SVG图标，支持MCP/Playwright/Puppeteer",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc index.ts --outDir . --target ES2020 --module ESNext --moduleResolution node --skipLibCheck",
    "test": "node index.js"
  },
  "keywords": [
    "iconfont",
    "svg",
    "icon",
    "download",
    "mcp",
    "playwright",
    "puppeteer"
  ],
  "author": "Super Client R",
  "license": "MIT",
  "optionalDependencies": {
    "playwright": "^1.40.0",
    "puppeteer": "^21.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "playwright": {
      "optional": true
    },
    "puppeteer": {
      "optional": true
    }
  }
}
